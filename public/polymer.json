{
  "entrypoint": "index.html",
  "basePath":"https://gdgspainsummit.firebaseapp.com/",
  "shell": "src/community-app.html",
  "fragments": [
    "src/community-contacts/community-contacts.html",
    "src/community-memes/community-memes.html",
    "src/community-settings/community-settings.html",
    "src/community-c4p/community-c4p.html",
    "src/community-login/community-login.html",
    "src/community-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
